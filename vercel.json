{
  "version": 2,
  "functions": {
    "api/screener.js": {
      "runtime": "@vercel/node"
    },
    "api/health.js": {
      "runtime": "@vercel/node"
    },
    "api/market-data.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "index/**",
      "use": "@vercel/static"
    }
  ]
}